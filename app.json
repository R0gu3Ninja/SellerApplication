{
  "expo": {
    "name": "LocalSeller",
    "slug": "localseller",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow to access your camera"
        }
      ]
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/local.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/local.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/local.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jayendrabhaskar.Homescreen",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/local.png"
    },
    "extra": {
      "eas": {
        "projectId": "411ca42f-c272-4f3d-9f48-1ae19ac5f31e"
      },
      "firebase": {
        "apiKey": "AIzaSyBRpGsKKV27WJdgmqhU1HMTA0ki8sDeIbQ",
        "authDomain": "signin-d4d32.firebaseapp.com",
        "projectId": "signin-d4d32",
        "storageBucket": "signin-d4d32.appspot.com",
        "messagingSenderId": "238728598242",
        "appId": "1:238728598242:web:f0fe21f4f10144a3ec84bb",
        "measurementId": "G-4H0QEER3V5"
      }
    },
    "owner": "jayendrabhaskar"
  }
}
